#include <bits/stdc++.h>
using namespace std;
int main()
{
  int m, n,i,j;
cout<< "no of rows is m:";
cin>>m;
cout<<"no of colums is n:";
cin>>n;
int A[m][n],B[m][n],C[m][n];
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
  cin>>A[i][j];
}
}
for(int i=0;i<m;i++)
{
for(int j=0;j<n;j++)
{
  cout<<A[i][j]<<" ";
  
 
}
cout<<endl;
}
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cin>>B[i][j];
    }
}
for(int i=0;i<m;i++)
{
for( int j=0;j<n;j++)
{
    cout<<B[i][j]<<" ";
}
cout<<endl;
}
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        C[i][j]=A[i][j]-B[i][j];
    }
}
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        cout<<C[i][j]<<" ";
    }
    cout<<endl;
}
}
